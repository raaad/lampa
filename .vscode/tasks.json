{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "watch",
      "type": "npm",
      "script": "watch",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "isBackground": true,
      "problemMatcher": [
        {
          "base": "$tsc",
          "background": {
            "activeOnStart": true,
            "beginsPattern": "> npx vite",
            "endsPattern": "built in(.*)$"
          }
        }
      ]
    }
  ]
}
